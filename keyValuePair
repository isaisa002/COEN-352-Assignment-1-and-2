package coen352.a1;



    //class to store object in <K,V> format

   public class keyValuePair<Key, ELEM> {
       private final Key k;
       private final ELEM element;

       //Constructors

       keyValuePair() {
           k = null;
           element = null;
       }

       keyValuePair(Key k, ELEM element) {
           this.k = k;
           this.element = element;
       }

       //get functions

       public Key key() {
           return k;
       }

       public ELEM value() {
           return element;
       }

   }
